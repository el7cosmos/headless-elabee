{
    "name": "el7cosmos/headless-elabee",
    "description": "Project template for Drupal 9 projects with a relocated document root",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/drupal",
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "package",
            "package": {
                "name": "w8tcha/ckeditor-codemirror-plugin",
                "version": "1.17.11",
                "type": "drupal-library",
                "extra": {
                    "installer-name": "ckeditor_codemirror"
                },
                "dist": {
                    "url": "https://github.com/w8tcha/CKEditor-CodeMirror-Plugin/releases/download/v1.17.11/CKEditor-CodeMirror-Plugin.zip",
                    "type": "zip"
                }
            }
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-apcu": "*",
        "ext-intl": "*",
        "ext-mbstring": "*",
        "ext-redis": "*",
        "composer/installers": "^1.9",
        "drupal/admin_toolbar": "^3.0",
        "drupal/codesnippet": "^1.7",
        "drupal/codetag": "^1.2",
        "drupal/core-composer-scaffold": "^9",
        "drupal/core-recommended": "^9",
        "drupal/file_delete_ui": "^1.0@alpha",
        "drupal/gin": "^3.0@alpha",
        "drupal/gin_login": "^1.0@RC",
        "drupal/gin_toolbar": "^1.0@beta",
        "drupal/next": "^1.0@beta",
        "drupal/pathauto": "^1.8",
        "drupal/redis": "^1.4",
        "drupal/s3fs": "^3.0@alpha",
        "drush/drush": "^10.3"
    },
    "require-dev": {
        "drupal/core-dev": "^9.0",
        "mglaman/phpstan-drupal": "^0.12.10",
        "phpspec/prophecy-phpunit": "^2.0",
        "phpstan/phpstan-deprecation-rules": "^0.12.6",
        "symfony/dotenv": "^5.1"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "replace": {
        "paragonie/random_compat": "*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-mbstring": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-intl-idn": "*",
        "symfony/polyfill-intl-normalizer": "*"
    },
    "autoload": {
        "psr-4": {
            "Elabee\\": "src"
        }
    },
    "autoload-dev": {
        "files": [
            "load.environment.php"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "apcu-autoloader": true,
        "sort-packages": true
    },
    "extra": {
        "drupal-scaffold": {
            "gitignore": true,
            "locations": {
                "web-root": "web/"
            }
        },
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/contrib/{$name}": ["type:drupal-drush"],
            "web/modules/custom/{$name}": ["type:drupal-custom-module"],
            "web/themes/custom/{$name}": ["type:drupal-custom-theme"]
        },
        "patches": {
            "drupal/simple_oauth": {
                "Update implementation": "patches/drupal/simple_oauth/update-implementation.patch"
            },
            "drupal/subrequests": {
                "Subrequest failed validation": "https://www.drupal.org/files/issues/2019-05-27/3029570-array-not-object.patch"
            },
            "drupal-ckeditor-libraries-group/codetag": {
                "Add class language none": "patches/drupal-ckeditor-libraries-group/codetag/add-class-language-none.patch"
            }
        }
    }
}
